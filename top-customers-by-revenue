              <svg width="18" height="18" viewBox="0 0 24 24" fill="none" style="vertical-align:middle;">
                <path d="M3 5a1 1 0 0 1 1-1h16a1 1 0 0 1 .8 1.6l-6.6 8.8V19a1 1 0 0 1-1.5.87l-3-2A1 1 0 0 1 9 17v-3.6L2.2 6.6A1 1 0 0 1 3 5z" stroke="#A0AEC0" stroke-width="2" fill="none"/>
