    // Clients Score Bar Chart
    var clientsScoreBarChart = echarts.init(document.getElementById('clientsScoreBarChart'));
    clientsScoreBarChart.setOption({
      xAxis: { type: 'value', axisLabel:{fontSize:14,color:'#4A5568', fontWeight: 500}, splitLine:{lineStyle:{color:'#f0f0f0'}}, name:'Client Score', nameLocation:'middle', nameGap:40, nameTextStyle:{color:'#4A5568', fontWeight: 500, fontSize: 14} },
      yAxis: { type: 'category', data: ['Olivia Bennett','Liam Carter','Sophia Turner','Noah Brooks','Ava Mitchell','Mason Reed','Isabella Hayes','Elijah Foster','Mia Sullivan','Lucas Perry','Charlotte Price','James Morgan','Amelia Ross','Benjamin Cooper','Harper Bailey'], axisLabel:{fontSize:14,color:'#4A5568', fontWeight: 500}, axisTick:{alignWithLabel:true}, axisLine:{lineStyle:{color:'#767070ff'}}, name:'Client', nameLocation:'middle', nameGap:110, nameTextStyle:{color:'#4A5568', fontWeight: 500, fontSize: 14} },
      series: [{
        data: [100,99,98,97,97,96,95,94,94,93,92,91,91,90,90],
        type: 'bar',
        itemStyle: { color: '#774ED9' },
        barWidth: 16,
        barCategoryGap: '30%',
        label: { show: true, position: 'right', fontSize:14, color:'#4A5568', fontWeight: 500, formatter: function(p){return p.value;} }
      }],
      grid: { left: 140, right: 40, top: 40, bottom: 70 },
      tooltip: { trigger: 'axis' }
    });
    // Clients by State Map (US Heatmap)
    var clientsByStateMap = echarts.init(document.getElementById('clientsByStateMap'));
    var stateData = [
      { abbr: 'TX', name: 'Texas', value: 160 },
      { abbr: 'OK', name: 'Oklahoma', value: 22 },
      { abbr: 'AR', name: 'Arkansas', value: 8 },
      { abbr: 'LA', name: 'Louisiana', value: 20 },
      { abbr: 'NM', name: 'New Mexico', value: 18 },
      { abbr: 'CA', name: 'California', value: 7 },
      { abbr: 'NY', name: 'New York', value: 23 },
      { abbr: 'FL', name: 'Florida', value: 5 },
      { abbr: 'IL', name: 'Illinois', value: 4 },
      { abbr: 'PA', name: 'Pennsylvania', value: 3 },
      { abbr: 'OH', name: 'Ohio', value: 2 },
      { abbr: 'GA', name: 'Georgia', value: 3 },
      { abbr: 'NC', name: 'North Carolina', value: 2 },
      { abbr: 'MI', name: 'Michigan', value: 2 }
    ];
    fetch('https://raw.githubusercontent.com/PublicaMundi/MappingAPI/master/data/geojson/us-states.json')
      .then(res => res.json())
      .then(usaJson => {
        echarts.registerMap('USA', usaJson);
        // Build lookup for both abbreviation and full name
        var stateLookup = {};
        stateData.forEach(s => {
          stateLookup[s.abbr] = s.value;
          stateLookup[s.name] = s.value;
        });
        var fullStateData = usaJson.features.map(function(f) {
          var abbr = f.properties.STATE_ABBR || f.properties.abbrev;
          var name = f.properties.name;
          var value = stateLookup[abbr] !== undefined ? stateLookup[abbr] : (stateLookup[name] !== undefined ? stateLookup[name] : 0);
          return { name: abbr || name, value: value };
        });
        clientsByStateMap.setOption({
          tooltip: {
            trigger: 'item',
            formatter: function(params) {
              return params.name + ': ' + (params.value !== undefined ? params.value : 0);
            }
          },
          visualMap: {
            min: 0,
            max: 160,
            left: 'right',
            top: 'center',
            text: ['High','Low'],
            calculable: true,
            inRange: { color: ['#6c63ff', '#e0e7ff'] }
          },
          series: [{
            name: 'USA',
            type: 'map',
            map: 'USA',
            roam: true,
            zoom: 1.0,
            label: {
              show: true,
              formatter: function(params) {
                return params.value !== undefined ? params.value : 0;
              },
              fontSize: 16,
              color: '#333'
            },
            data: fullStateData
          }]
        });
      });
  </script>
